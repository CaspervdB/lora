{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "Location schema",
  "type": "object",
  "required": [
    "nodeID",
    "description",
    "locationname",
    "capacity"
  ],
  "properties": {
    "nodeID": {
      "type": "integer",
      "minimum": 0
    },
    "description": {
      "type": "string",
      "minLength": 0
    },
    "locationname": {
      "type": "string",
      "minLength": 0
    },
    "capacity": {
      "type": "integer",
      "minimum": 0
    }
  }
}